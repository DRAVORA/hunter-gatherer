{
  "expo": {
    "name": "Atavia",
    "slug": "atavia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0808"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dravora.atavia",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.dravora.atavia",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F0808"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": ["expo-sqlite"]
  }
}
